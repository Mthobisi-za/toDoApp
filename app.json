{
    "expo": {
        "name": "toDoApp",
        "slug": "toDoApp",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./app/assets/to-do-list.png",
        "splash": {
            "image": "./app/assets/check.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./app/assets/to-do-list.png",
                "backgroundColor": "#FFFFFF"
            }
        },
        "web": {
            "favicon": "./app/assets/to-do-list.png"
        }
    }
}